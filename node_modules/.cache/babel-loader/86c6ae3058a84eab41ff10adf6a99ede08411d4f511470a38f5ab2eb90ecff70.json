{"ast":null,"code":"import Swal from 'sweetalert2';\nimport axios from 'axios';\nexport default {\n  data: () => ({\n    Toast: Swal.mixin({\n      toast: true,\n      position: 'top-end',\n      showConfirmButton: false,\n      timer: 1500,\n      timerProgressBar: true,\n      didOpen: toast => {\n        toast.addEventListener('mouseenter', Swal.stopTimer);\n        toast.addEventListener('mouseleave', Swal.resumeTimer);\n      }\n    })\n  }),\n  methods: {\n    deleteVideo(id) {\n      Swal.fire({\n        title: 'Are you sure?',\n        icon: 'question',\n        showCancelButton: true,\n        confirmButtonColor: '#7FBA5E',\n        cancelButtonColor: '#d33',\n        confirmButtonText: 'Yes, delete it!'\n      }).then(res => {\n        if (res.isConfirmed) {\n          axios.delete('/videos/' + id + '/').then(() => {\n            this.Toast.fire({\n              icon: 'success',\n              title: 'Successfully deleted'\n            });\n            this.$emit('delete-success');\n          }).catch(err => {\n            console.log(err);\n            this.Toast.fire({\n              icon: 'error',\n              title: 'Something went wrong'\n            });\n          });\n        }\n      });\n    }\n  },\n  props: {\n    headers: {\n      type: Array,\n      required: true\n    },\n    videos: {\n      type: Array,\n      required: true\n    }\n  }\n};","map":{"version":3,"names":["Swal","axios","data","Toast","mixin","toast","position","showConfirmButton","timer","timerProgressBar","didOpen","addEventListener","stopTimer","resumeTimer","methods","deleteVideo","id","fire","title","icon","showCancelButton","confirmButtonColor","cancelButtonColor","confirmButtonText","then","res","isConfirmed","delete","$emit","catch","err","console","log","props","headers","type","Array","required","videos"],"sources":["src/components/example/Tables/VideoTable.vue"],"sourcesContent":["<template>\r\n  <v-card\r\n      elevation=\"0\"\r\n      color=\"white\"\r\n      width=\"100%\"\r\n      style=\"border-radius: 10px; margin-top: 20px\"\r\n  >\r\n    <v-simple-table\r\n        dense\r\n    >\r\n      <template v-slot:default>\r\n        <thead>\r\n        <tr>\r\n          <th\r\n              v-for=\"(item, i) in headers\"\r\n              :key=\"i\"\r\n          >\r\n            {{ item }}\r\n          </th>\r\n        </tr>\r\n        </thead>\r\n        <tbody>\r\n        <tr\r\n            v-for=\"(item, i) in videos\"\r\n            :key=\"i\"\r\n            :style=\"i % 2 == 0 ? 'background-color: #F2F2F7': ''\"\r\n            class=\"table-row\"\r\n\r\n        >\r\n          <td @click=\"$emit('show-full-info', item)\">{{ i + 1 }}</td>\r\n          <td @click=\"$emit('show-full-info', item)\">{{ item.nameTm }}</td>\r\n          <td @click=\"$emit('show-full-info', item)\">{{ item.nameRu }}</td>\r\n          <td style=\"width: 100px\">\r\n            <v-btn icon x-small>\r\n              <v-icon color=\"orange\" size=\"20\">mdi-pencil-outline</v-icon>\r\n            </v-btn>\r\n            <v-btn icon x-small @click=\"deleteVideo(item.id)\">\r\n              <v-icon color=\"red\" size=\"20\">mdi-delete-outline</v-icon>\r\n            </v-btn>\r\n          </td>\r\n        </tr>\r\n        </tbody>\r\n      </template>\r\n    </v-simple-table>\r\n  </v-card>\r\n</template>\r\n\r\n\r\n<script>\r\nimport Swal from 'sweetalert2';\r\nimport axios from 'axios';\r\nexport default {\r\n  data: () => ({\r\n    Toast: Swal.mixin({\r\n      toast: true,\r\n      position: 'top-end',\r\n      showConfirmButton: false,\r\n      timer: 1500,\r\n      timerProgressBar: true,\r\n      didOpen: (toast) => {\r\n        toast.addEventListener('mouseenter', Swal.stopTimer)\r\n        toast.addEventListener('mouseleave', Swal.resumeTimer)\r\n      },\r\n    }),\r\n  }),\r\n  methods: {\r\n    deleteVideo(id) {\r\n      Swal.fire({\r\n        title: 'Are you sure?',\r\n        icon: 'question',\r\n        showCancelButton: true,\r\n        confirmButtonColor: '#7FBA5E',\r\n        cancelButtonColor: '#d33',\r\n        confirmButtonText: 'Yes, delete it!'\r\n      })\r\n          .then((res) => {\r\n            if (res.isConfirmed) {\r\n              axios.delete('/videos/' + id + '/')\r\n                  .then(() => {\r\n                    this.Toast.fire({\r\n                      icon:'success',\r\n                      title: 'Successfully deleted'\r\n                    })\r\n                    this.$emit('delete-success')\r\n                  })\r\n                  .catch((err) => {\r\n                    console.log(err)\r\n                    this.Toast.fire({\r\n                      icon:'error',\r\n                      title: 'Something went wrong'\r\n                    })\r\n                  })\r\n            }\r\n          })\r\n    }\r\n  },\r\n  props: {\r\n    headers: {\r\n      type: Array,\r\n      required: true\r\n    },\r\n    videos: {\r\n      type: Array,\r\n      required: true\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.table-row:hover {\r\n  background-color: #0088FF !important;\r\n  color: white;\r\n  cursor: pointer;\r\n}\r\n</style>"],"mappings":"AAiDA,OAAAA,IAAA;AACA,OAAAC,KAAA;AACA;EACAC,IAAA,EAAAA,CAAA;IACAC,KAAA,EAAAH,IAAA,CAAAI,KAAA;MACAC,KAAA;MACAC,QAAA;MACAC,iBAAA;MACAC,KAAA;MACAC,gBAAA;MACAC,OAAA,EAAAL,KAAA;QACAA,KAAA,CAAAM,gBAAA,eAAAX,IAAA,CAAAY,SAAA;QACAP,KAAA,CAAAM,gBAAA,eAAAX,IAAA,CAAAa,WAAA;MACA;IACA;EACA;EACAC,OAAA;IACAC,YAAAC,EAAA;MACAhB,IAAA,CAAAiB,IAAA;QACAC,KAAA;QACAC,IAAA;QACAC,gBAAA;QACAC,kBAAA;QACAC,iBAAA;QACAC,iBAAA;MACA,GACAC,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,WAAA;UACAzB,KAAA,CAAA0B,MAAA,cAAAX,EAAA,QACAQ,IAAA;YACA,KAAArB,KAAA,CAAAc,IAAA;cACAE,IAAA;cACAD,KAAA;YACA;YACA,KAAAU,KAAA;UACA,GACAC,KAAA,CAAAC,GAAA;YACAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;YACA,KAAA3B,KAAA,CAAAc,IAAA;cACAE,IAAA;cACAD,KAAA;YACA;UACA;QACA;MACA;IACA;EACA;EACAe,KAAA;IACAC,OAAA;MACAC,IAAA,EAAAC,KAAA;MACAC,QAAA;IACA;IACAC,MAAA;MACAH,IAAA,EAAAC,KAAA;MACAC,QAAA;IACA;EACA;AACA